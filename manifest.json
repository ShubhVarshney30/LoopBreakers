{
    "manifest_version": 3,
    "name": "Tab Switch Counter",
    "version": "1.0",
    "description": "Counts how many times you switch tabs",
    "permissions": ["tabs", "storage", "notifications"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }